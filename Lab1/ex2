import numpy as np
def ex2():
  n = 1000
  lambdas = [1, 2, 5, 10]

  X1 = np.random.poisson(lam=1, size=n)
  X2 = np.random.poisson(lam=2, size=n)
  X3 = np.random.poisson(lam=5, size=n)
  X4 = np.random.poisson(lam=10, size=n)

  lambda_choices = np.random.choice(lambdas, size=n)
  X_randomize = np.random.poisson(lam=lambda_choices)

print(ex2())


